{
  "name": "ct-autocompletion",
  "version": "1.0.0",
  "description": "Provides medical autocompletion and relevant synonyms",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon --harmony --ignore _scripts/ --ignore scripts/ app.js",
    "serve": "(cd demo/ && http-server -s -p 4040 -o)",
    "test": "jest --config=jest.json --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:CTcue/ctAutocompletion.git"
  },
  "engines": {
    "node": ">=14.17"
  },
  "keywords": [
    "autocompletion",
    "medical synonyms"
  ],
  "dependencies": {
    "koa": "2.13.4",
    "koa-router": "10.1.1",
    "koa-body": "4.2.0",
    "@koa/cors": "3.1.0",
    "@elastic/elasticsearch": "7",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "jest": "27.4.7",
    "http-server": "14.0.0",
    "nodemon": "2.0.15",
    "supertest": "6.1.6",
    "split2": "4.1.0",
    "through2": "4.0.2",
    "elasticsearch-writable-stream": "2.0.1"
  },
  "license": "Proprietary",
  "private": true
}
