{
  "name": "CTcUMLS",
  "version": "0.0.1",
  "description": "Autocomplete and expander api for UMLS terms",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --harmony app.js",
    "production": "forever start -l $(date +%g_%m_%d_%H_%M_%S).log -o $(date +%g_%m_%d_%H_%M_%S).log -e $(date +%g_%m_%d_%H_%M_%S).log --spinSleepTime 10000 --minUptime 5000 -c \"node --harmony\" app.js --ctCumls",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CTcue/CTcUMLS.git"
  },
  "keywords": [
    "umls",
    "autosuggest",
    "autocomplete",
    "suggest",
    "complete",
    "expander",
    "ctcue"
  ],
