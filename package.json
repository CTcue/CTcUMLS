{
  "name": "ct-autocompletion",
  "version": "1.0.0",
  "description": "Autocompletion and relevant synonyms from UMLS",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon --harmony --ignore _scripts/ --ignore scripts/ app.js",
    "serve": "(cd demo/ && http-server -s -p 4040 -o)",
    "test": "cross-env NODE_ENV=e2e && jest --config=jest.json --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:CTcue/ctAutocompletion.git"
  },
  "keywords": [
    "umls",
    "suggestions",
    "autocompletion"
  ],
  "dependencies": {
    "@elastic/elasticsearch": "7",
    "elasticsearch-writable-stream": "2.0.1",
    "koa": "2.7.0",
    "koa-cors": "0.0.16",
    "koa-router": "5.4.0",
    "lodash": "4.17.21",
    "minimist": "1.2.3",
    "moment": "2.24.0",
    "request": "2.88.0",
    "split2": "3.1.1",
    "through2": "3.0.1"
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "jest": "23.6.0",
    "http-server": "0.12.3",
    "nodemon": "1.19.1",
    "supertest": "4.0.2"
  },
  "license": "Proprietary",
  "private": true
}
