  "bugs": {
    "url": "https://github.com/CTcue/CTcUMLS/issues"
  },
  "homepage": "https://github.com/CTcue/CTcUMLS",
  "dependencies": {
    "co": "^4.0.0",
    "co-body": "^1.0.0",
    "co-mysql": "^1.0.0",
    "elasticsearch": "^3.0.0",
    "koa": "^0.12.1",
    "koa-cors": "0.0.13",
    "koa-helmet": "0.0.1",
    "koa-json": "^1.1.1",
    "koa-trail": "0.0.2",
    "lodash": "^2.4.1",
    "mysql": "^2.5.3",
    "request-json": "^0.5.0",
    "sift-string": "0.0.2",
    "sugar": "^1.4.1"
