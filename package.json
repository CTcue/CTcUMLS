{
  "name": "ct-autocompletion",
  "version": "0.1.1",
  "description": "Relevant UMLS terms based on user input",
  "main": "app.js",
  "scripts": {
    "prestart": "yarn install --silent",
    "start": "nodemon --harmony --ignore _scripts/ --ignore scripts/ app.js",
    "serve": "(cd demo/ && http-server -s -p 4040 -o)",
    "test": "set PORT=4088 && mocha --harmony -R spec --recursive ./test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:CTcue/ctAutocompletion.git"
  },
  "keywords": [
    "umls",
    "autosuggest",
    "autocomplete",
    "suggest",
    "complete"
  ],
  "author": "CTcue",
  "homepage": "https://github.com/CTcue/ctAutocompletion",
  "dependencies": {
    "cheerio": "^0.22.0",
    "co-monk": "1.0.0",
    "elasticsearch": "14.1.0",
    "elasticsearch-writable-stream": "^2.0.0",
    "koa": "2.5.0",
    "koa-cors": "0.0.16",
    "koa-router": "5.4.0",
    "lodash": "^4.15.0",
    "minimist": "^1.2.0",
    "moment": "^2.15.0",
    "mongodb": "^2.2.10",
    "monk": "3.1.2",
    "neo4j": "^2.0.0-RC2",
    "neo4j-batch-index-stream": "0.0.1",
    "request": "^2.74.0",
    "split2": "^2.1.0",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "supertest": "*",
    "mocha": "*",
    "nodemon": "1.15.1",
    "http-server": "0.11.1"
  },
  "license": "Proprietary",
  "private": true
}
