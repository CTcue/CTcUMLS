{
  "name": "ct-autocompletion",
  "version": "1.0.0",
  "description": "Provides medical autocompletion and relevant synonyms",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon --harmony --ignore _scripts/ --ignore scripts/ app.js",
    "serve": "(cd demo/ && http-server -s -p 4040 -o)",
    "test": "cross-env NODE_ENV=e2e && jest --config=jest.json --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:CTcue/ctAutocompletion.git"
  },
  "engines": {
    "node": ">=14.17"
  },
  "keywords": [
    "autocomplete",
    "autocompletion",
    "synonyms"
  ],
  "dependencies": {
    "@elastic/elasticsearch": "7",
    "elasticsearch-writable-stream": "2.0.1",
    "koa": "2.7.0",
    "@koa/cors": "3.1.0",
    "koa-router": "5.4.0",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "jest": "23.6.0",
    "http-server": "0.12.3",
    "nodemon": "2.0.15",
    "supertest": "6.1.6",
    "split2": "4.1.0",
    "through2": "4.0.2"
  },
  "license": "Proprietary",
  "private": true
}
